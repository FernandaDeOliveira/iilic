@model IEnumerable<iilic.Core.Consulta>
    <div class="content-wrapper">
        <section class="content-header">
            <h1>
                Iilic
                <small></small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="~/Administrador/IndexADM"><i class="fa fa-dashboard"></i> Index</a></li>

            </ol>
        </section>
        <section class="content">

            <!-- Principal -->
            <div class="box box-solid">
                <div class="box-header">
                    <div class="row">
                        <div class="col-lg-12">
                            <!-- Mudando essa area muda o conteudo do site -->
                            <div class="container">
                                <div class="row">
                                  <!--  <div class="col-lg-8 col-lg-offset-2 text-center">-->
                                    <div class="col-md-offset-4 col-md-4">
                                        <!-- Principal -->
                                        <!--  <div style="padding-top:120px">   <div class="col-xs-12 col-xs-offset-1 col-sm-10 col-sm-offset-2 col-md-4 col-md-offset-4 ">-->
                                        <div class="login panel panel-primary">
                                            <div class="panel-heading" style="font-size:medium">Consultas do dia</div>
                                            <div class="panel-body">
                                                <form action="CriarLogTER" method="post">
                                                    <fieldset>
                                                        <div class="form-group">

                                                            @foreach (var consulta in Model)
                                                            {

                                                                <div class="form-group col-lg-12">
                                                                    <label>Hora da consulta: @consulta.hora</label>

                                                                </div>
                                                                <div class="form-group col-lg-12">
                                                                    <label>Nome do paciente: @consulta.paciente.nomePaciente</label>

                                                                </div>
                                                                <div class="form-group col-lg-12">
                                                                    <label>Nome do terapeuta: @consulta.terapeuta.nomeMed</label>

                                                                </div>

                                                            }
                                                            <div>
                                                                <button type="submit" class="btn btn-primary btn-lg" aria-label="Salvar">
                                                                    <span class="glyphicon glyphicon-floppy-disk"></span>
                                                                </button>
                                                                <button type="reset" class="btn btn-danger btn-lg" aria-label="Limpar">
                                                                    <span class="glyphicon glyphicon-remove"></span>
                                                                </button>

                                                            </div>
                                                        </div>
                                                    </fieldset>

                                                </form>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>

                            <!-- Mudando essa area muda o conteudo do site -->
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>