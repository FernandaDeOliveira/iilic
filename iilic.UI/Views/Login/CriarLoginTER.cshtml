@{
    ViewBag.Title = "Cadastro Terapeuta";
}
}
 &lt;
<script language="JavaScript" type="text/javascript" src="~/scripts/MascaraValidacao.js"></script>
<script type="text/javascript">
   $(function(){
      $(".close").click(function(){
         $("#myAlert").alert('close');
      });
   });
</script>
}
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <meta charset="utf-8" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.theme.css" rel="stylesheet" />
    <script src="~/scripts/jquery.js"></script>
    <script src="~/scripts/jquery-ui.js"></script>
</head>

<div class="container">
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2 text-center">
            @if (@ViewBag.login != null)
            {
                <div id="myAlert" class="alert alert-warning alert-dismissible fade in" role="alert">
                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                    <h3>@ViewBag.login</h3>
                </div>
            }
            <!-- Principal -->
            <!--  <div style="padding-top:120px">   <div class="col-xs-12 col-xs-offset-1 col-sm-10 col-sm-offset-2 col-md-4 col-md-offset-4 ">-->
            <div class="login panel panel-primary">
                <div class="panel-heading" style="font-size:medium">Digite seus dados para cadastro</div>
                <div class="panel-body">
                    <form action="CriarLogTER" method="post" name="form1">
                        <fieldset>
                            <div class="form-group">

                                <div class="form-group col-lg-12">
                                    <label>Nome *</label>
                                    <input class="form-control" type="text" name="nomeMed" required>
                                </div>

                                <div class="form-group  col-lg-12">
                                    <label>CPF  *</label>
                                    <input class="form-control" type="text" name="cpfMed" onBlur="ValidarCPF(form1.cpfMed);"
                                               onKeyPress="MascaraCPF(form1.cpfMed);" maxlength="14" required>
                                </div>
                                <div class="form-group  col-lg-4">
                                    <label>Data de nascimento *</label>
                                    <input class="form-control" type="date" name="dataNascMed" required>
                                </div>

                                <div class="form-group  col-lg-8">
                                    <label>Sexo *</label>
                                    <div class="form-group">

                                        <label><input type="radio" name="sexo" value="masculino" required>Masculino</label>
                                        <label><input type="radio" name="sexo" value="feminino" required>Feminino</label>

                                    </div>
                                </div>
                                <div class="form-group  col-lg-12">
                                    <label>Telefone *</label>
                                    <input class="form-control" type="text" name="telefone" onKeyPress="MascaraTelefone(form1.telefone);" maxlength="15" required>
                                </div>

                                <div class="form-group col-lg-12">
                                    <label> Email</label>
                                    <input class="form-control" type="text" name="email" required>
                                </div>
                                <div class="form-group col-lg-12">
                                    <label> CRM</label>
                                    <input class="form-control" type="text" name="crmM" required>
                                </div>
                                <div class="form-group col-lg-12">
                                    <label> Login</label>
                                    <input name="acesso.login" class="form-control input-md" type="text" required>
                                </div>
                            </div>
                            <div class="form-group col-lg-12">
                                <div class="col-md-20">
                                    <label> Senha</label>
                                    <input name="acesso.senha" class="form-control input-md" type="password" required>
                                </div>
                            </div>
                            <div class="form-group col-lg-12">
                                <div>
                                    <button type="submit" class="btn btn-primary btn-lg" aria-label="Salvar">
                                        <span class="glyphicon glyphicon-floppy-disk"></span>
                                    </button>
                                    <button type="reset" class="btn btn-danger btn-lg" aria-label="Limpar">
                                        <span class="glyphicon glyphicon-remove"></span>
                                    </button>

                                </div>
                            </div>
                        </fieldset>

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>


