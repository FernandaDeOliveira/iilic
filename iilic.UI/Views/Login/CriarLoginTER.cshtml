
@{
    ViewBag.Title = "CriarLoginTER";
}
}
<div class="container">
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2 text-center">

            <!-- Principal -->
            <!--  <div style="padding-top:120px">   <div class="col-xs-12 col-xs-offset-1 col-sm-10 col-sm-offset-2 col-md-4 col-md-offset-4 ">-->
            <div class="login panel panel-primary">
                <div class="panel-heading" style="font-size:medium">Digite seus dados para cadastro</div>
                <div class="panel-body">
                    <form action="CriarLogTER" method="post">
                        <fieldset>
                            <div class="form-group">

                                <div class="form-group col-lg-12">
                                    <label>Nome *</label>
                                    <input class="form-control" type="text" name="nomeMed" required>
                                </div>

                                <div class="form-group  col-lg-12">
                                    <label>CPF  *</label>
                                    <input class="form-control" type="text" name="cpfMed" required>
                                </div>
                                <div class="form-group  col-lg-4">
                                    <label>Data de nascimento *</label>
                                    <input class="form-control" type="date" name="dataNascMed" required>
                                </div>

                                <div class="form-group  col-lg-8">
                                    <label>Sexo *</label>
                                    <div class="form-group">

                                        <label><input type="radio" name="sexo" value="masculino" required>Masculino</label>
                                        <label><input type="radio" name="sexo" value="feminino" required>Feminino</label>

                                    </div>
                                </div>
                                <div class="form-group  col-lg-12">
                                    <label>Telefone *</label>
                                    <input class="form-control" type="text" name="telefone" required>
                                </div>

                                <div class="form-group col-lg-12">
                                    <label> Email</label>
                                    <input class="form-control" type="text" name="email" required>
                                </div>
                                <div class="form-group col-lg-12">
                                    <label> CRM</label>
                                    <input class="form-control" type="text" name="crmM" required>
                                </div>
                                <div class="form-group col-lg-12">
                                    <label> Login</label>
                                    <input name="acesso.login" class="form-control input-md" type="text" required>
                                </div>
                            </div>
                            <div class="form-group col-lg-12">
                                <div class="col-md-20">
                                    <label> Senha</label>
                                    <input name="acesso.senha" class="form-control input-md" type="password" required>
                                </div>
                            </div>
                            <div class="form-group col-lg-12">
                                <div>
                                    <button type="submit" class="btn btn-primary btn-lg" aria-label="Salvar">
                                        <span class="glyphicon glyphicon-floppy-disk"></span>
                                    </button>
                                    <button type="reset" class="btn btn-danger btn-lg" aria-label="Limpar">
                                        <span class="glyphicon glyphicon-remove"></span>
                                    </button>

                                </div>
                            </div>
                        </fieldset>

                    </form>
                </div>
            </div>
        </div>
    </div>
</div>


